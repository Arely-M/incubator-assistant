<div class="page-content p-5" id="content">
	<div class="card-title">
		<div class="row">
			<div class="col-sm-10">
				<h4>Lote 1</h4>
			</div>
		</div>
	</div>
	<div class="separator"></div>
	<div class="cardBox-egg">
		{{#each eggs as |egg|}}
			<div class="card">
				<img class="card-img" src="/img/egg.png" alt="Card image cap" />
				<div class="card-body">
					<h5 class="card-title" style="text-align: center;">Huevo #{{egg.number}}</h5>
					<a href="/eggs/{{egg._id}}/edit" class="btn"><i class="fa fa-eye" aria-hidden="true"></i></a>

				</div>
			</div>

		{{/each}}
	</div>
</div>