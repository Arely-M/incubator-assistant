{{> nav/navigation}}
<!-- page-content -->
<div class="page-content p-5" id="content">
	{{#each role}}    
		{{#ifCond _id '==' ../user}}                        
			{{#ifCond role '==' "Administrador"}}                   
			<!-- cards -->
			<div class="cardBox" style="padding-bottom: 30px;">
				<div class="card">
					<div>
						<div class="numbers">{{../users}}</div>
						<div class="cardName">Usuarios</div>
					</div>
					<div class="iconBx">
						<ion-icon name="people-circle-outline"></ion-icon>
					</div>
				</div>
				<div class="card" onmouseover="white()" onmouseout="gray()">
					<div>
						<div class="numbers">{{../lots}}</div>
						<div class="cardName">Incubaciones</div>
					</div>
					<div class="iconBx">
						<img id="incubator" width="49px" src="./img/incubator.png" title="Incubadora">
					</div>
				</div>
				<div class="card">
					<div>
						<div class="numbers">{{../candlings}}</div>
						<div class="cardName">Ovoscopias</div>
					</div>
					<div class="iconBx">
						<ion-icon name="egg-outline"></ion-icon>
					</div>
				</div>
				<!--<div class="card">
					<div>
						<div class="numbers">1,504</div>
						<div class="cardName">DDDD DDDD</div>
					</div>
					<div class="iconBx">
						<ion-icon name="eye-outline"></ion-icon>
					</div>
				</div>-->
			</div>
			{{/ifCond}}
		{{/ifCond}}
	{{/each}}

	
